{
  "env": {
      "browser": true,
      "node": true,
      "commonjs": true,
      # "es6": true,
      "prototypejs": true,
      "serviceworker": true
  },
  "extends": "eslint:recommended",
  # "parser": "esprima",
  "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module",
      # "ecmaFeatures": {
      #   "jsx": true,
      #   "globalReturn": true
      #   }
  },
  "rules": {
      "no-console": 0,
      "indent": [ "error", 2 ],
      "quotes": [ "error", "single" ],
      "semi": [ "error", "always" ]
  },
  "globals": {
    "DBHelper": false,
    "L": true,
    "newMap": true,
    "self": true,
    "error": true
    }
}
